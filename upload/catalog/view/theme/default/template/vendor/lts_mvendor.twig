{{ header }}
<div id="container" class="container">
  <ul class="breadcrumb">
    {% for breadcrumb in breadcrumbs %}
    <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
    {% endfor %}
  </ul>
 
    <div id="content" class="col-sm-12">
      <div class="dashboard-wrap">
        <h1 class="primary-heading">{{ heading_title }}</h1>
          {% if vendors %}
          <div class="row">
          {% for vendor in vendors %}
          <div class="col-lg-4 col-md-4 col-sm-6">
            <div class="vendor-wrap">
              <a href="{{ vendor.href }}"><img src="{{ vendor.banner }}" alt="" title="" class="img-responsive"></a>
              <div class="vendor-info">
                <div class="row">
                  <div class="col-sm-12">
                    <div class="vendor-img clearfix">
                      {% if display_vendor_image %}
                      <div class="img-wrap"><a href="{{ vendor.href }}"><img src="{{ vendor.image }}" alt="" title="" class="img-responsive img-circle"></a></div>
                      {% endif %}
        
                    </div>
                    <div class="vendor-detail">
                      <ul class="list-unstyled">
                        <li class="store-name"><a href="{{ vendor.href }}">{{ vendor.store_name }}</a></li>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          {% endfor %}
        </div>
         <div class="row">
        <div class="col-sm-6 text-left">{{ pagination }}</div>
        <div class="col-sm-6 text-right">{{ results }}</div>
      </div>
        {% else %}
        <p>{{ text_empty }}</p>
        {% endif %}
      </div>
      {{ content_bottom }}</div>
</div>

{{ footer }}
