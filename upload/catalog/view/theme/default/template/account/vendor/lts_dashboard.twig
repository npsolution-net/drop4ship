{{ header }}
<div class="container">
  <ul class="breadcrumb">
    {% for breadcrumb in breadcrumbs %}
      <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
      {% endfor %}
  </ul>
  {% if success %}
    <div class="alert alert-success alert-dismissible"><i class="fa fa-check-circle"></i> {{ success }}</div>
  {% endif %}

  <div class="row">   
    {{ lts_column_left }} 
    <div id="content" class="col-sm-9">
      {{ mobile_menu }}   
      <div class="lts-dashboard-wrap">
        <div class="row">
          
          <div class="col-sm-12">
            <div class="lts-dashboard-right">
              <div class="row">
                <div class="col-sm-6">

                  <div class="lts-status-box">
                    <ul class="list-unstyled">
                      <li>
                        <div class="title">{{ text_total_products}} <a href="{{add_product}}">{{ text_add_new_product }}</a></div>
                        <div class="count highlight">{{  product_total }}</div>
                      </li>
                    </ul>
                  </div>

                 
                </div>

                <div class="col-sm-6">   



                   <div class="lts-status-box">
                    <ul class="list-unstyled">
                      <li>
                        <div class="title">{{ text_oders }} <a href="{{ view_order }}">{{ text_view_all }}</a></div>
                        <div class="count highlight">
                          {{ order_total }}
                        </div>
                      </li>
                    </ul>
                  </div>

                  

              {#    <div class="lts-status-box">
                    <ul class="list-unstyled">
