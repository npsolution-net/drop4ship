
{{ header }}
<div class="container">
  <ul class="breadcrumb">
    {% for breadcrumb in breadcrumbs %}
      <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
      {% endfor %}
  </ul>
  {% if success %}
    <div class="alert alert-success alert-dismissible"><i class="fa fa-check-circle"></i> {{ success }}</div>
  {% endif %}

  <div class="row">   
    {{ lts_column_left }}
    <div id="content" class="col-sm-9">
      {{ lts_mobile_menu }}   
      <div class="lts-dashboard-wrap">
        <div class="row">
          <div class="col-sm-12 lts-subscriptions">
            <div class="panel panel-default">
              <div class="panel-heading clearfix"><span>{{ text_confirmation }}</span>
              </div>
            </div>

            <div class="panel-body">
              <div class="col-sm-12 lts-subscription">
                <div class="col-sm-12 inovice-content">
                  <div class="col-sm-6 admin-store">
                    <p><strong>{{ text_admin_store }} : </strong>{{ config_name }}</p>
                    <p><strong>{{ text_admin_address }} : </strong>{{ config_address }}</p>
                    <p><strong>{{ text_admin_telephone }} : </strong>{{ config_telephone }}</p>
                    <p><strong>{{ text_admin_email }} : </strong>{{ config_email }}</p>
                    <p><strong>{{ text_admin_website }} : </strong>{{ config_website }}</p>
                  </div>
                  <div class="col-sm-6 vendor-store">
                    <p><strong>{{ text_vendor_store }} :</strong>{{ vendor_store_name }}</p>
                    <p><strong>{{ text_vendor_address }}</strong>{{ vendor_address }}</p>
                    <p><strong>{{ text_vendor_telephone }}: </strong>{{ vendor_telephone }}</p>
                    <p><strong>{{ text_vendor_email }}: </strong>{{ vendor_email }}</p>
                    <p><strong>{{ text_vendor_website }}: </strong>{{ config_website }}</p>
                  </div>
                </div>

                 <div class="col-sm-12 ">
                    <div class="col-sm-9">
                      <p><strong>{{ name }}</strong> </p>
                      <p><strong>{{ text_start_date }} : </strong>{{ date_added }}</p>
                      <p><strong>{{ text_expire_date }} : </strong>{{ date_expire }}</p>
                    </div>

                     <div class="col-sm-3 text-right">
                        {# <span></span> #}
                      </div>
                  </div>
                             
                    <div class="col-sm-12 text-right p-0">
                       <table class="table table-bordered">
                          <tbody>
                            <tr>
                              <td style="width: 90%;"><strong>{{ text_join_fee }}</strong><br /></td><td style="width: 0%;"><strong>{{ join_fee }}</strong>
                             </td>
                            </tr>

                             <tr>
                              <td style="width: 10%;"><strong>{{ text_subscription_fee }}</strong><br /></td><td style="width: 0%;"><strong>{{  subscription_fee }}</strong>
                             </td>
                            </tr>   

                             <tr>
                              <td style="width: 10%;"><strong>{{ text_total_grand }}</strong><br /></td><td style="width: 0%;"><strong>{{  total }}</strong>
                             </td>
                            </tr>


                          </tbody>
                        </table>
                     
                    </div>
                  </div>
                                  
                          



          </div>
        </div>
      </div>
    </div>
  </div>
</div>
{{ footer }}

