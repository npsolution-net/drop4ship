<aside id="column-left" class="col-sm-3">
  <div class="lts-dashboard-wrap">
    <div class="navbar-header"><span id="vendor-menus" class="visible-xs">Vendor Menus</span>
      <button type="button" class="btn btn-navbar navbar-toggle" data-toggle="collapse" data-target=".navbar-ex2-collapse"><i class="fa fa-bars"></i></button>
    </div>

    <div class="collapse navbar-collapse navbar-ex2-collapse">
      <ul class="list-unstyled">
         {# <ul class="list-unstyled" id="menu"> #}
     {% set i = 0 %}
     {% for menu in menus %}
     <li id="{{ menu.id }}">
         {% if menu.href %} <a href="{{ menu.href }}"><i class="fa {{ menu.icon }} fw"></i> {{ menu.name }}</a>{% else %}<a href="#collapse{{ i }}" data-toggle="collapse" class="parent collapsed"><i class="fa {{ menu.icon }} fw"></i> {{ menu.name }}</a>{% endif %}
         {% if menu.children %}
           <ul id="collapse{{ i }}" class="collapse" style="margin-bottom: 0px; padding: 0px; list-style-type: none;">
             {% set j = 0 %}
             {% for children_1 in menu.children %}
                {% if children_1.href %}
                   <a href="{{ children_1.href }}"> <i class="fa {{ children_1.icon }} fw"></i>  {{ children_1.name }}</a>
                 {% endif %}
               {% set j = j + 1 %}
             {% endfor %}
           </ul>
         {% endif %}
       </li>
     {% set i = i + 1 %}
     {% endfor %}
        </ul>
      {# </ul> #}
    </div> 






  </div> 
</aside> 